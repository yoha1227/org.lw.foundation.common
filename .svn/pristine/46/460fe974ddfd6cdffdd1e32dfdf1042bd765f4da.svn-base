<div class="AffairEntryDetailForm">
		<form data-dojo-type="dijit/form/Form">
			<div style="width: 100%; height: auto;">
				<fieldset style="border: 2px groove threedface; margin: 2px; padding: 0.75em; height: auto;">
					<legend>事件详细信息</legend>
					<table border="1" class="table-list" style="width: 100%;">
						<colgroup>
							<col></col>
						</colgroup>
						<tbody>
							<tr>
								<td>事件主题</td>
							</tr>
							<tr>
								<td><input type="text" data-dojo-type="dijit/form/TextBox" name="subject"
									style="width: 100%;" class="font16"></input>
								</td>
							</tr>
							<tr>
								<td>事件描述</td>
							</tr>
							<tr>
								<td><textarea type="text" data-dojo-type="dijit/form/SimpleTextarea" trim="false"
										name="description" style="width: 100%; height: 110px; overflow-y: hidden;" class="font16"></textarea></td>
							</tr>
							<tr>
								<td class="disabled">处理结果</td>
							</tr>
							<tr>
								<td><textarea type="text" data-dojo-type="dijit/form/SimpleTextarea" trim="false"
										name="resolveResult" style="width: 100%; height: 108px; overflow-y: hidden;" class="font16"></textarea></td>
							</tr>
						</tbody>
					</table>
				</fieldset>
			</div>

		</form>
</div>

