/*详情样式*/
.opinionDetails {
    width: 100%;
}

.opinionDetails .head {
    font-size: 14px;
    font-weight: bold;
    font-family: "宋体";
    color: #fff;
    line-height: 30px;
    background-image: url(../images/tabline3.gif);
    text-indent: 20px;
}

.opinionDetails .Display {
    background-color: #fff;
    margin: 0px;
    padding: 10px;
    color: #000;
}
.opinionDetails .Display .divContainer .jumpLink li {
    background-image: url(../images/edit_05.gif);
    border: 1px solid #4081af;
    margin: 5px;
    padding: 2px;
    color: #000;
    text-align: center;
}
.opinionDetails .Display .divContainer .jumpLink li a:visited {
     color: #5188d3;
}
.opinionDetails .container Display ul {
    margin: 15px;
}
/**
.opinionDetails .Display div {
    background: url(weather.jpg) no-repeat -5px 0px;
    margin: 0px 5px 0px 5px;
    background-color: #5ea6eb;
}
*/
.opinionDetails .Display h3 {
    border: 1px solid #AFAFAF;
    font-size: 12px;
    padding: 5px;
    color: #000;
    font-weight: bold;
    margin: 0px;
    background-image: url(../images/tabline2.gif);
}

.opinionDetails .Display .title {
    font-size: 12px;
    font-weight: bold;
    color: #585920;
}

.opinionDetails .Display .datalist {
    /* 表格边框 */
    border-collapse: collapse;
    font-family: Arial;
    background-color: #bebebe; /* 表格背景色 */
    font-size: 12px;
    width: 100%;
}

.opinionDetails .Display .datalist th {
    border: 1px solid #999999; /* 行名称边框 */
    background-color: #f4f4f4; /* 行名称背景色 */
    color: #5188d3; /* 行名称颜色 */
    text-align: center;
    padding: 5px;
    background-image: url(../images/tabline1.gif);
    background-position: center center;
    background-repeat: repeat-x;
}

.opinionDetails .Display .datalist td {
    border: 1px solid #AFAFAF; /* 单元格边框 */
    text-align: left;
    background-color: #fff;
    color: #888888;
    padding: 4px;
}

.opinionDetails .Display .datalist td.altrow {
    background-color: #fafafa;
}

.opinionDetails .Display .datalist tr.altrow {
    background-color: #F0F0F0; /* 隔行变色 */
}
.opinionDetails .Display .datalist td.selected {/* 选中,变色 */
    font-weight:bold;
    color: #0f0f0f; 
}


