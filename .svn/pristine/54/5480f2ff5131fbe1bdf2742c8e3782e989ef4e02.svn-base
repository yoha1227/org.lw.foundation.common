@CHARSET "UTF-8";
.navigableTabContainer{
}
.navigableTabContainer .dijitTabContainerTop-tabs .dijitTabContent{
    padding:0;
    background: none;
    border-right-width:0;
    border-left-width:0;
}
.navigableTabContainer .dijitTabContainerTop-tabs .dijitTabChecked .dijitTabContent{
    padding-bottom:0; 
    padding-top:0;
    background: none;
} 
.navigableTabContainer .dijitTabContainerTop-tabs .dijitTab{
    position:relative;
    padding-top: 0;
    top: 0;
}
