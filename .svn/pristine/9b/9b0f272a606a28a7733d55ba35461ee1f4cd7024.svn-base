<div class="CustomerInfoViewer" style="${style}">
	<form data-dojo-type="dijit/form/Form"
		style="width: 100%; height: 100%;">
		<fieldset style="border: 2px groove threedface; margin: 2px; padding: 0.75em; height: 100%">
			<legend>客户信息</legend>
			<table border="1"
				style="border-collapse: collapse; table-layout: fixed; width: 100%; height: 100%;" data-dojo-attach-point="tableNode" class="table-list">
				<tbody>
					<tr>
						<td>
							<button data-dojo-type="dijit/form/Button" data-dojo-attach-point="ButtonNode" type="button">
							 选择客户
							</button>
						</td>
					</tr>
					<tr>
						<td>名称</td>
					</tr>
					<tr>
						<td>
							<span>
							<input type="text" data-dojo-type="dijit/form/TextBox" name="name"
							style="width: 100%;"></input></span>
						</td>
					</tr>
					<tr>
						<td>联系电话</td>
					</tr>
					<tr>
						<td><input type="text" data-dojo-type="dijit/form/TextBox" name="tel"
							style="width: 100%;"></input>
						</td>
					</tr>
					<tr>
						<td>电子邮件</td>
					</tr>
					<tr>
						<td><input type="text" data-dojo-type="dijit/form/TextBox" name="email"
							style="width: 100%;"></input>
						</td>
					</tr>
					<tr>
						<td>公司</td>
					</tr>
					<tr>
						<td><input type="text" data-dojo-type="dijit/form/TextBox" name="company"
							style="width: 100%;"></input>
						</td>
					</tr>
					<tr>
						<td>部门</td>
					</tr>
					<tr>
						<td><input type="text" data-dojo-type="dijit/form/TextBox" name="department"
							style="width: 100%;"></input>
						</td>
					</tr>
				</tbody>
			</table>
		</fieldset>
	</form>

</div>

