<div class="SimpleEntityEditorForm">
	<form data-dojo-type="dijit/form/Form"
		style="width: 100%">
		<div style="float: left; width: 40%; height: 500px;">
			<fieldset
				style="border: 2px groove threedface; margin: 2px; padding: 0.75em;">
				<legend>Simple Entity Editor</legend>
				<table border="1" class="table-list"
					style="width: 100%; height: 300px;">
					<colgroup>
						<col></col>
						<col></col>
					</colgroup>
					<tbody>
						<tr>
							<td>Entity Id</td>
							<td><span data-dojo-type="dijit/form/TextBox"
								name="id"></span>
							</td>
						</tr>
						<tr>
							<td>Entity Name</td>
							<td><input type="text" data-dojo-type="dijit/form/TextBox"
								name="name" style="width: 100%;"></input>
							</td>
						</tr>
						<tr>
							<td>Entity Description</td>
							<td><input type="text" data-dojo-type="dijit/form/TextBox"
								name="description" style="width: 100%;"></input>
							</td>
						</tr>
						<tr>
							<td class="disabled">Entity Disabled Field</td>
							<td><input type="text" data-dojo-type="dijit/form/TextBox"
								name="disabledField" style="width: 100%;" disabled=disabled></input></td>
						</tr>
					</tbody>
				</table>
			</fieldset>
		</div>
	</form>
</div>

